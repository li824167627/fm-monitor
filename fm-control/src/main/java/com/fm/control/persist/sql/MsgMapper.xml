<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fm.control.persist.sql.MsgMapper">

	<insert id="insert">
		INSERT INTO msg
		(
		sn,pm2p5,co2,tvoc,humidity,temperature,pm10 )
		VALUES
		(
		#{sn},#{pm2p5},#{co2},#{tvoc},#{humidity},#{temperature},#{pm10} )
	</insert>

</mapper>